  "version": "0.2.0",
  "configurations": [

      {
          "type": "gdb",
          "request": "attach",
          "name": "Attach to Pintos (threads)",
          "executable": "${workspaceFolder}/src/threads/build/kernel.o",
          "target": ":1234",
          "remote": true,
          "cwd": "${workspaceRoot}", 
          "gdbpath": "/pintos/src/utils/pintos-gdb"
      },
      {
        "type": "gdb",
        "request": "attach",
        "name": "Attach to Pintos (userprog)",
        "executable": "${workspaceFolder}/src/userprog/build/kernel.o",
        "target": ":1234",
        "remote": true,
        "cwd": "${workspaceRoot}", 
        "gdbpath": "/pintos/src/utils/pintos-gdb"
    },
    {
      "type": "gdb",
      "request": "attach",
      "name": "Attach to Pintos (vm)",
      "executable": "${workspaceFolder}/src/vm/build/kernel.o",
      "target": ":1234",
      "remote": true,
      "cwd": "${workspaceRoot}", 
      "gdbpath": "/pintos/src/utils/pintos-gdb"
  }
  ]
}